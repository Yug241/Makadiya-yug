#include <iostream>

using namespace std;

int main() {
    int num1, num2;
    
    // Input first number
    cout << "Enter first number: ";
    cin >> num1;
    
    // Input second number
    cout << "Enter second number: ";
    cin >> num2;
    
    // Calculate sum
    int sum = num1 + num2;
    
    // Print sum
    cout << "Sum: " << sum << endl;
    
    return 0;
}
